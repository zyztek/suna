modules = ["nodejs-20", "python-3.12", "web", "docker", "nix", "gcloud", "bash"]
run = "git clone https://github.com/kortix-ai/suna.git && cd suna && python setup.py && python start.py"

[nix]
channel = "stable-25_05"

[deployment]
run = ["sh", "-c", "cd backend && python -m uvicorn api:app --host 0.0.0.0 --port 5000"]
build = ["sh", "-c", "cd frontend && npm install && npm run build"]
